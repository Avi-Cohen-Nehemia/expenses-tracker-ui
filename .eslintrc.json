{
    "env": {
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        // "react/jsx-sort-props": ["warn", {
        //     "callbacksLast": false,
        //     "shorthandFirst": false,
        //     "shorthandLast": false,
        //     "ignoreCase": true,
        //     "noSortAlphabetically": false,
        //     "reservedFirst": false
        // }]
    }
}
